{
  "name": "elder-dungeons",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "build": "prisma generate && next build",
    "build:worker": "opennextjs-cloudflare build",
    "dev": "prisma generate && next dev",
    "start": "next start",
    "lint": "eslint",
    "import:spell-reference": "node scripts/import-spell-reference.mjs",
    "sync:spell-reference": "node scripts/import-spell-reference.mjs",
    "repair:spell-ptbr": "node scripts/repair-spell-ptbr-encoding.mjs",
    "preview": "opennextjs-cloudflare build && opennextjs-cloudflare preview",
    "deploy": "opennextjs-cloudflare build && opennextjs-cloudflare deploy",
    "deploy:worker": "npm run build:worker && CLOUDFLARE_ACCOUNT_ID=$(printf %s \"$CLOUDFLARE_ACCOUNT_ID\" | tr -d '\"') wrangler versions upload --config wrangler.jsonc --compatibility-date 2026-02-20",
    "upload": "opennextjs-cloudflare build && opennextjs-cloudflare upload",
    "cf-typegen": "wrangler types --env-interface CloudflareEnv cloudflare-env.d.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.883.0",
    "@opennextjs/cloudflare": "^1.16.5",
    "@prisma/adapter-pg": "^7.4.0",
    "@prisma/client": "^7.4.0",
    "@radix-ui/react-label": "^2.1.7",
    "csv-parse": "^5.6.0",
    "jsonwebtoken": "^9.0.2",
    "mysql2": "^3.17.2",
    "next": "16.1.6",
    "openai": "^6.8.1",
    "pg": "^8.16.3",
    "prisma": "^7.4.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/node": "^20",
    "@types/pg": "^8.15.5",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "tailwindcss": "^4",
    "typescript": "^5",
    "wrangler": "^4.67.0"
  }
}